# kustomize/components/moon/base/deployment.yaml v1.0.0
# Base deployment for the moon component.
apiVersion: apps/v1
kind: Deployment
metadata:
  name: moon
spec:
  replicas: 1
  selector:
    matchLabels:
      app: moon
  template:
    metadata:
      labels:
        app: moon
    spec:
      containers:
        - name: moon
          image: "nginxdemos/hello:plain-text"
          ports:
            - containerPort: 80